---

import Layout from "../layouts/Layout.astro";

import Header from "../components/structural/Header.astro";

import Landing from "../components/structural/Hero.vue";
import About from "../components/structural/About.astro";

import Experience from "../components/structural/Experience.vue";

import Tecnologies from "../components/structural/Tecnologies.vue"

import Projects from "../components/structural/Projects.vue";

import Footer from "../components/structural/Footer.astro";

---

<Layout title="Bienvenido a mi web">

    <Header />

    <Landing client:load />

    <About />

    <main class="flex flex-col my-16 gap-5">
        <Experience client:load />
        <Tecnologies client:load />
        <Projects client:load />
    </main>
    <Footer />
</Layout>
